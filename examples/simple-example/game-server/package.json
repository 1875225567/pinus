{
    "name": "pinus-simple-example-game-server",
    "version": "1.2.19",
    "private": false,
    "bin": {
        "$": "./dist/bin/app.js"
    },
    "main": "./dist/app",
    "scripts": {
        "build": "tsc",
		"start": "yarn run build && node dist/index",
		"lint": "node --max_old_space_size=4096 ../../../node_modules/tslint/bin/tslint --format prose -c ../../../tslint.json lib/**/*.ts test/**/*.ts",
		"fix-lint": "node --max_old_space_size=4096 ../../../node_modules/tslint/bin/tslint --format prose -c ../../../tslint.json lib/**/*.ts test/**/*.ts --fix",
		"test": "yarn run build",
		"cov": "nyc mocha",
		"ci": "yarn run test",
		"gen-api-ref": "node ../../../node_modules/typedoc/bin/typedoc --mode file --hideGenerator --excludeExternals --ignoreCompilerErrors --out ../../../run/site/public/api-reference/pinus-loader lib/"

    },
    "dependencies": {
        "@types/bluebird": "^3.5.19",
        "@types/node": "^8.5.2",
        "bluebird": "^3.5.1",
        "pinus": "^1.2.19",
        "reflect-metadata": "^0.1.10",
        "source-map-support": "^0.5.0",
        "typescript": "^2.6.1"
    }
}
